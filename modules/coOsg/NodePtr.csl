/*
	Wraps an osg::Node ref pointer from OSG Library.
*/
@co.Include( "osg/Node" )
native class NodePtr
{
	<c++
		// Forward Declarations:
		namespace osg {
			class Node;
			template<class T> class ref_ptr;
		} // namespace osg
		namespace coOsg {
			typedef osg::ref_ptr<osg::Node> NodePtr;
		} // namespace coOsg
	c++>
};
