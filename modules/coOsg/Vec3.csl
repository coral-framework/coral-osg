/*
	Wraps a Vec3d.
 */
native class Vec3 ( <osg/Vec3d> osg::Vec3d )
{
	double x;
	double y;
	double z;

	void length( out double length );
	void normalize();
	void dot( in Vec3 other, out double result );
	void cross( in Vec3 other );
	void add( in Vec3 other );
	void sub( in Vec3 other );
	void equals( in Vec3 other, out bool isEqual );

	void mulScalar( in double scalar );
	void divScalar( in double scalar );
};
