/*
	Wraps a Quaternion.
 */
native class Quat ( <osg/Quat> osg::Quat )
{
	double x;
	double y;
	double z;
	double w;

	void zeroRotation();
	void preMult( in Quat other );
	void postMult( in Quat other );
	void makeRotate( in double radians, in Vec3 axis );
	void fromMatrix( in Matrix matrix );
	void toMatrix( out Matrix matrix );
	void getRotate( out double angle, out Vec3 axis );
	void slerp( in double t, in Quat from, in Quat to );
};
